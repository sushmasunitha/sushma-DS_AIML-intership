import pandas as pd

df = pd.read_csv("customer_orders.csv")

print("Initial data types:\n")
print(df.dtypes)

df["Price"] = (
    df["Price"]
    .str.replace("$", "", regex=False)   
    .astype(float)                       # convert to float
)

df["Date"] = pd.to_datetime(df["Date"])

print("\nUpdated data types:\n")
print(df.dtypes)
